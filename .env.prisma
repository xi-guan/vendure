# Prisma ORM Configuration
# Copy this file to .env to enable Prisma ORM

# Enable Prisma ORM support
VENDURE_ENABLE_PRISMA=true

# Set ORM mode (typeorm | prisma)
VENDURE_ORM_MODE=prisma

# Enable query logging (for development)
VENDURE_PRISMA_LOG_QUERIES=false

# Enable performance metrics (for benchmarking)
VENDURE_PRISMA_PERFORMANCE_METRICS=false

# Database connection
DATABASE_URL="postgresql://vendure:password@localhost:5432/vendure"
